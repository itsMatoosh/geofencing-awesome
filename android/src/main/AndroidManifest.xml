<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="io.flutter.plugins.geofencing">
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <application>
        <receiver android:enabled="true" android:exported="true" android:name="io.flutter.plugins.geofencing.GeofencingBroadcastReceiver"/>
        <receiver android:enabled="true" android:exported="true" android:name="io.flutter.plugins.geofencing.GeofencingRebootBroadcastReceiver"/>
        <service android:name="io.flutter.plugins.geofencing.GeofencingService"
            android:permission="android.permission.BIND_JOB_SERVICE" android:exported="true"/>
    </application>
</manifest>
